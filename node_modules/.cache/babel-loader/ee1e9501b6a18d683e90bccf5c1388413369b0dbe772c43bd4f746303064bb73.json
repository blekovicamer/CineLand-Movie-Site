oming\"\r\n      );\r\n\r\n      this.upcomingMovies = response.data.results.slice(1, 6);\r\n      console.log(this.upcomingMovies);\r\n    },\r\n  },\r\n};\r\n\r\n  </script>\r\n  \r\n  <style>\r\n  .carousel-3d-container figure {\r\n    margin: 0;\r\n  }\r\n  \r\n  .carousel-3d-container figcaption {\r\n    position: absolute;\r\n    background-color: rgba(0, 0, 0, 0.5);\r\n    color: #fff;\r\n    bottom: 0;\r\n    position: absolute;\r\n    bottom: 0;\r\n    padding: 15px;\r\n    font-size: 12px;\r\n    min-width: 100%;\r\n    box-sizing: border-box;\r\n  }\r\n  \r\n  .next span,\r\n  .prev span {\r\n    color: white;\r\n  }\r\n  </style>"],"mappings":"AA2BE,SAASA,UAAU,EAAEC,KAAI,QAAS,iBAAiB;AACnD,eAAe;EACfC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,cAAc,EAAE;IAClB,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,mBAAmB,CAAC,CAAC;EAC5B,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,mBAAmBA,CAAA,EAAG;MAC1B,MAAME,QAAO,GAAI,MAAM,IAAI,CAACC,KAAK,CAACC,GAAG,CACnC,6CACF,CAAC;MAED,IAAI,CAACN,cAAa,GAAII,QAAQ,CAACL,IAAI,CAACQ,OAAO,CAACC,KAAK,CAAC,CAAC,EAAE,CAAC,CAAC;MACvDC,OAAO,CAACC,GAAG,CAAC,IAAI,CAACV,cAAc,CAAC;IAClC;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}